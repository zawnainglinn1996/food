<odoo>
     <data noupdate="1">
         <record id="seq_submission_of_quotation" model="ir.sequence">
            <field name="name">Submission Quotation</field>
            <field name="code">submission.quotation</field>
            <field name="prefix">SQ-%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
             <field eval="True" name="use_date_range"/>
            <field name="range_reset">yearly</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>