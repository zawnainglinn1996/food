<odoo>
    <data noupdate="1">
         <record id="bank_statement_code" model="ir.sequence">
            <field name="name">Account Bank Statement Code</field>
            <field name="code">bank.statement.code</field>
            <field name="prefix">%(year)s/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
