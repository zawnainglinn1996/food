<odoo>
	<data>
		<record id="view_res_users_form_inherit_1" model="ir.ui.view">
			<field name="name">res.users.inherit.new.form</field>
			<field name="model">res.users</field>
			<field name="inherit_id" ref="base.view_users_form"/>
			<field name="arch" type="xml">
				<xpath expr="//page[@name='access_rights']/group[1]" position="after">
					<group/>
					<group>
						<field name="is_concurrent_user"/>
					</group>
				</xpath>
			</field>
		</record>
	</data>
</odoo>